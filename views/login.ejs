<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>请登录</title>
    <script src="lib/jquery.min.js"></script>
    <link rel="stylesheet" href="bootstrap/dist/css/bootstrap.min.css">
    <script src="bootstrap/dist/js/bootstrap.js"></script>
    <style type="text/css">
        div{
            width: 500px;
            height: 50px;
            margin: 10px auto;
        }
        .submit{
            width: 250px;
            height: 30px;
        }
        button{
            margin-left: 80px;
        }
    </style>
</head>
<body>
 <%if(bok==true)  {%>
<div class="input-group input-group-lg">
    <span class="input-group-addon" id="sizing-addon1">请输入管理员账户</span>
    <input type="text" class="form-control" placeholder="Username" aria-describedby="sizing-addon1" id="username">
</div>

<div class="input-group input-group-lg">
    <span class="input-group-addon" id="sizing-addon1">请输入管理员密码</span>
    <input type="password" class="form-control" placeholder="PassWord" aria-describedby="sizing-addon1" id="password">
</div>

<div class="submit" id="submit">
    <button>登录</button>
</div>



<%} else  {%>

 <nav class="navbar navbar-inverse" role="navigation">
     <div class="container ">
         <div class="navbar-header none">
             <a class="navbar-brand" href="###">所有微信玩家数据展示</a>
         </div>
     </div>
 </nav>
<div class="widget widget-table">

    <div class="widget-header">
        <i class="icon-th-list"></i>

    </div> <!-- /widget-header -->

    <div class="widget-content">

        <table class="table table-striped table-bordered">
            <thead>
            <tr>
                <th>收货人</th>
                <th>收货地址</th>
                <th>电话</th>
                <th>邮箱</th>
                <th>微信昵称</th>
                <th>头像</th>
                <th>游戏分数</th>
                <th>游戏排名</th>
            </tr>
            </thead>

            <tbody id="tBody">
            <%for(var i=0; i< data.length; i++){%>
            <tr>
                <td><%=data[i].realname%></td>
                <td><%=data[i].address%></td>
                <td><%=data[i].phoneNumber%></td>
                <td><%=data[i].email%></td>
                <td><%=data[i].wxName%></td>
                <td><img  style="width: 50px ; height: 50px" src=<%=data[i].headimgurl%> alt=""></td>
                <td><%=data[i].highestScore%></td>
                <td><%=(i+1)%></td>
            </tr>
            <%}%>
            </tbody>
        </table>

    </div> <!-- /widget-content -->

</div>
<%}%>



</body>
<script src="lib/jquery.min.js"></script>
<script type="text/javascript">
    var btn = document.getElementById('submit');
    var username='';
    var psw='';
    btn.onclick=function (e) {
         username=document.getElementById('username').value;
         psw=document.getElementById('password').value;
        if(username.length>0 && psw.length>0){
            var user={
             username:username,
             psw:psw
            }
         $.ajax({
             url:'data',
             type:'post',
             data:user,
             dataType:'json',
             success:function (result) {
                 if(result.status==false){
                  alert('账号或密码错误')
                 }
             }
         })

        }else{
            alert('账户或密码不能为空')
            return false;
        }

    }




</script>
</html>